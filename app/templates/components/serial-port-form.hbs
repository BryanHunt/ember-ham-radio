{{#paper-select placeholder="Port" model=port}}
  {{#each ports as |port|}}
    {{#paper-option value=port}}{{port}}{{/paper-option}}
  {{/each}}
{{/paper-select}}
{{#paper-button action="refreshPorts"}}Refresh Ports{{/paper-button}}
{{#if port}}
  {{#paper-button action="readData" }}Read Data{{/paper-button}}
{{/if}}
